{
  "name": "vite-typescript-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm:backend\" \"npm:dev\"",
    "backend": "concurrently \"npm:tsc-watch\" \"npm:nodemon-dist-watch\"",
    "nodemon-dist-watch": "npm run killBackend && nodemon --watch backendDist backendDist/index.js",
    "tsc-watch": "cd backend && tsc --watch",
    "dev": "npm run killVite && vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "killVite": "node _kill/killVitePort.js",
    "killBackend": "node _kill/killBackendport.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "kill-port-process": "^3.2.1",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-restart": "^0.4.1"
  }
}
